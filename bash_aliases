
# Git
alias g="git"
alias ga="git add"
alias gc="git commit -v"
alias gd="git diff"
alias gds="git diff --staged"
alias gst="git status"
alias gco="git checkout"
alias glg="git log --stat"

# ls
alias la="ls -a"
#alias ll="ls -l"
alias las="ls -lash"

alias e="nvim"

alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'
